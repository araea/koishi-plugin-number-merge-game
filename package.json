  "version": "1.1.3",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "koishi": {
    "service": {
      "required": [
        "monetary",
        "database",
        "puppeteer"
      ],
      "optional": [
        "markdownToImage"
      ]
    }
  },
  "license": "MIT",
  "author": "Nawyjx <3373167460@qq.com>",
  "homepage": "https://www.npmjs.com/package/koishi-plugin-number-merge-game?activeTab=readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/araea/koishi-plugin-number-merge-game.git"
  },
  "keywords": [
    "chatbot",
    "koishi",
    "plugin",
    "2048",
    "2048Game",
    "game",
    "游戏",
    "数字合并小游戏"
  ],
  "peerDependencies": {
    "koishi": "^4.16.8"
  },
  "devDependencies": {
    "koishi-plugin-markdown-to-image-service": "^1.1.3",
    "koishi-plugin-monetary": "^0.1.3",
    "koishi-plugin-puppeteer": "^3.7.3"
  }
}
